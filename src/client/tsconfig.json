{
    "compilerOptions": {
      "target": "ESNext",
      "strict": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "module": "ESNext",
      "types": ["@citizenfx/client", "@types/node"],
      "lib": ["ESNext"]
    },
    "include": ["./**/*"],
    "exclude": ["**/node_modules", "**/__tests__/*"]
}